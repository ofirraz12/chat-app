{
  "expo": {
    "name": "Exerra",
    "slug": "betagymapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-forground.png",
        "backgroundImage": "./assets/images/icon-background1.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "URL_backend": "https://chat-app-production-abd9.up.railway.app/api",
      "LLM_MODEL": "groq",
      "appState": "prod",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8fc27180-0989-4489-be1d-0e62300eeb33"
      }
    }
  }
}
